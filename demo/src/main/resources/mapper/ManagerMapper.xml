<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.ManagerMapper">
    <select id="getmanager" resultType="String">
        select * from manager where name =#{username}
    </select>
    <select id="getalluser" resultType="com.example.demo.dom.User">
        select * from user
    </select>
</mapper>
